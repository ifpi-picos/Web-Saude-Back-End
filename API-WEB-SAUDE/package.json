{
	"name": "web-saude-back-end",
	"version": "1.0.0",
	"outDir": "dist",
	"description": "Template de configurações para utilizar o NodeJS em conjunto com o TypeScript",
	"main": "src/server.ts",
	"include": [
		"src/**/*"
	],
	"scripts": {
		"start": "node dist/src/server.js",
		"start:dev": "ts-node-dev src/server.ts",
		"lint": "eslint ./src --ext .ts",
		"lint-fix": "eslint ./src .test --ext .ts --fix",
		"build": "npx tsc",
		"format": "prettier --write \"**/*.{ts,json,}\"",
		"test": "jest --projects ./test --forceExit --runInBand"
	},
	"dependencies": {
		"@types/bcrypt": "^5.0.0",
		"@types/body-parser": "^1.19.2",
		"@types/compression": "^1.7.2",
		"@types/config": "^3.3.0",
		"@types/cookie-parser": "^1.4.4",
		"@types/cors": "^2.8.13",
		"@types/express": "^4.17.17",
		"@types/express-session": "^1.17.8",
		"@types/jsonwebtoken": "^9.0.2",
		"@types/mongoose": "5.11.97",
		"@types/morgan": "^1.9.4",
		"@types/node": "^20.4.2",
		"@types/swagger-ui-express": "^4.1.4",
		"@types/typescript": "^2.0.0",
		"@typescript-eslint/eslint-plugin": "^6.0.0",
		"@typescript-eslint/parser": "^6.0.0",
		"bcrypt": "^5.1.0",
		"body-parser": "^1.20.2",
		"compression": "^1.7.4",
		"config": "^3.3.9",
		"cookie-parser": "^1.4.6",
		"cors": "^2.8.5",
		"dotenv": "^16.3.1",
		"eslint": "^8.45.0",
		"express": "^4.18.2",
		"express-session": "^1.17.3",
		"helmet": "^7.0.0",
		"jsonwebtoken": "9.0.1",
		"mongoose": "7.3.4",
		"morgan": "^1.10.0",
		"swagger-ui-express": "^5.0.0",
		"ts-node-dev": "^2.0.0",
		"typescript": "^5.1.6",
		"typescript-eslint": "^0.0.1-alpha.0"
	},
	"devDependencies": {
		"@types/dotenv": "^8.2.0",
		"@types/helmet": "^4.0.0",
		"@types/jest": "^29.5.5",
		"@types/supertest": "^2.0.15",
		"jest": "^29.7.0",
		"supertest": "^6.3.3",
		"ts-jest": "^29.1.1"
	}
}
